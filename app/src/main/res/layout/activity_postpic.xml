<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PostPicActivity">

    <TextView
        android:id="@+id/success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="192dp"
        android:text="You successfully took a picture!"
        android:textSize="22dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="137dp"
        android:layout_height="81dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="550dp"
        android:onClick="openMainActivity"
        android:text="Go back"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/json1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="669dp"
        android:text="JSON placeholder 1"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/json2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="640dp"
        android:text="JSON placeholder 2"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/json3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="609dp"
        android:text="JSON placeholder 3"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/percentage_error"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="6570dp"
        android:layout_marginBottom="248dp"
        android:text="percentage_error"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/newRoll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginBottom="349dp"
        android:text="new JSON value 3"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/newAzimuth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="408dp"
        android:text="new JSON value 1"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/newTilt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="379dp"
        android:text="new JSON value 2"
        android:textSize="15dp" />

</RelativeLayout>